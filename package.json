{
  "name": "clone-tabnews",
  "version": "1.0.0",
  "description": "Clone do tabnews.com.br para o curso.dev",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "lint:check": "prettier --check .",
    "lint:write": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "docker:up": "docker compose --file ./infra/compose.yaml up -d",
    "docker:down": "docker compose --file ./infra/compose.yaml down",
    "docker:stop": "docker compose --file ./infra/compose.yaml stop",
    "docker:check": "docker ps -a"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "next": "^14.2.5",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "prettier": "^3.3.3"
  }
}
